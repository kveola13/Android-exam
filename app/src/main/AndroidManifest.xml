<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="no.woact.kveola13">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".application.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".application.ActivitySinglePlayer"
            android:label="Single Player"
            />

        <activity android:name=".application.ActivityMultiPlayer"
            android:label="Multi Player"/>
        <activity android:name=".application.Player1WinScreen"
            android:label="WINNER"/>
        <activity android:name=".application.Player2WinScreen"
            android:label="WINNER"/>
        <activity android:name=".application.PlayerDrawScreen"
            android:label="DRAW"/>
        <activity android:name=".application.MatchHistory"
            android:label="Match History"/>
    </application>

</manifest>